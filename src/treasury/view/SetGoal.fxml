      <Button focusTraversable="false" layoutX="135.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="48.0" prefWidth="77.0" styleClass="bigbutton" stylesheets="@GraphicController.css" text="%cancel">
