            <Font name="AppleGothic Regular" size="14.0" />
         </font></TextField>
      <CheckBox fx:id="agreeToTerms" focusTraversable="false" layoutX="131.0" layoutY="458.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="290.0" stylesheets="@GraphicController.css" text="%agree" textFill="#ffaa2b">
