            <Font name="AppleGothic Regular" size="14.0" />
         </font></TextField>
      <TextField fx:id="confirmEmailField" focusTraversable="false" layoutX="131.0" layoutY="311.0" prefHeight="40.0" prefWidth="244.0" promptText="%conf" styleClass="signuptextfields" stylesheets="@GraphicController.css">
         <font>
            <Font name="AppleGothic Regular" size="14.0" />
         </font></TextField>
      <PasswordField fx:id="passwordField" focusTraversable="false" layoutX="131.0" layoutY="361.0" prefHeight="40.0" prefWidth="244.0" promptText="%password" styleClass="signuptextfields" stylesheets="@GraphicController.css">
