<AnchorPane id="AnchorPane" fx:id="anchorPane" onMouseClicked="#unfocus" prefHeight="600.0" prefWidth="500.0" styleClass="signuptheme" stylesheets="@GraphicController.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="treasury.view.SignUpController">
   <children>
      <Button focusTraversable="false" layoutX="131.0" layoutY="151.0" mnemonicParsing="false" onAction="#handleSignUpWithFacebook" prefHeight="37.0" prefWidth="239.0" style="-fx-background-color: #3B5998; -fx-border-color: #15237c;" text="%withfb" textFill="#d3d3d3" />
      <Label layoutX="241.0" layoutY="206.0" text="%or" textFill="#ffaa2b">
