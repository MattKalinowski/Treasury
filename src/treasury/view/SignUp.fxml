      <Button fx:id="signupButton" alignment="CENTER" focusTraversable="false" layoutX="190.0" layoutY="494.0" mnemonicParsing="false" onAction="#handleSignUp" pickOnBounds="false" prefHeight="50.0" prefWidth="134.0" scaleShape="false" styleClass="signupbutton" stylesheets="@GraphicController.css" text="%signup" textAlignment="CENTER">
